{
  "source": {
    "include": [
      "src/js",
      "index.js"
    ],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": false,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "opts": {
    "encoding": "utf8",
    "readme": "./README.md",
    "destination": "./docs",
    "recurse": true,
    "verbose": true,
    "template": "node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "theme": "light"
    }
  }
}